{
  "canisters": {
    "backend": {
      "metadata": [
        {
          "name": "candid:service"
        }
      ],
      "candid": "src/backend/backend.did",
      "package": "backend",
      "type": "custom",
      "wasm": "target/wasm32-wasi/release/backend-ic.wasm",
      "build": [ "bash build.sh" ]
    },
    "frontend": {
      "dependencies": [
        "backend"
      ],
      "frontend": {
        "entrypoint": "src/frontend/src/index.html"
      },
      "source": [
        "src/frontend/assets",
        "dist/frontend/"
      ],
      "type": "assets"
    }
  },
  "dfx": "0.22.0-beta.0",
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "ic": {
      "providers": ["https://mainnet.dfinity.network"],
      "type": "persistent"
    }
  },
  "output_env_file": ".env",
  "version": 1
}
